    "@types/node": "^22.5.4",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-node": "^2.1.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
